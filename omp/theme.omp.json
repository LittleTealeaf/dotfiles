{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "_old": [

      {
        "type": "prompt",
        "alignment": "left",
        "newline": true,
        "segments": [{
          "type": "session",
          "style": "plain",
          "template": "{{ if .SSHSession }}\uF817 {{ end }}{{ .UserName }} "
        },{
          "type": "path",
          "style": "plain",
          "properties": {
            "style": "full"
          },
          "template": "\u276f {{ .Path }} \u276f"
        },
        {
          "foreground_templates_": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f77622{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#e43b44{{ end }}",
            "{{ if gt .Ahead 0 }}#2ce8f5{{ end }}",
            "{{ if gt .Behind 0 }}#f77622{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} \u276f",
          "type": "git"
        },
          {
            "style": "plain",
            "template": "",
            "type": "text"
          }
        ]
      }
    ],
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "path",
            "style": "plain",
            "properties": {
              "style": "full"
            },
            "template": "{{ .Path }} \u276f"
          }
        ]
      },
      {
        "type": "rprompt",
        "alignment": "right",
        "segments": [
          {
            "properties": {
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "plain",
            "template": "\u276e {{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} \u276f",
            "type": "git"
          }
        ]
      }
    ],
    "final_space": true,
    "version": 2
  }
